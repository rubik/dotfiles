snippet p
	import ipdb;ipdb.set_trace()
snippet e
	${1} = ${2}
snippet i
	import ${1:module}
snippet f
	from ${1:module} import ${2:*}
snippet wh
	while ${1:condition}:
		${2:# code...}
snippet for
	for ${1:needle} in ${2:haystack}:
		${3:# code...}
snippet .
	self.
# if __name__ == '__main__':
snippet ifm
	if __name__ == '__main__':
		${1:main()}
# __magic__
snippet _
	__${1:init}__${2}
